<!DOCTYPE html>
<html>
  <head>
	<!--Import Google Icon Font-->
	<link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
      <!--Import materialize.css-->
      <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/css/materialize.min.css">
       <script src="https://code.jquery.com/jquery-3.1.0.js"></script>
      <!--Let browser know website is optimized for mobile-->
      <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
      <script src="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.4.0/Chart.min.js"></script>
      <script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/js/materialize.min.js"></script>
  </head>


  <body>
	<!-- navbar -->
	<div class="navbar-fixed">
		<nav>
			<div class="nav-wrapper blue">
				<a href="#" class="brand-logo center">Teacher Registration</a>

				<ul id="nav-mobile" class="right hide-on-med-and-down">
					<li><a href="/landing"><i class="material-icons">subdirectory_arrow_left</i></a></li>
				  </ul>
				<a href="" data-target="slide-out" class="sidenav-trigger"><i class="material-icons">menu</i></a>		
			</div>
		</nav>
		</div>
		
		<!-- content -->

		<div class="content bg">
		<div class="container">
		<div class="row">
			<div class="col s12">
				<h4 class="grey-text">New Teacher Registration</h4>
			</div>
			<div class="col s12 m6 l12">
				<div class="card card-bg grey-text">
					<div class="card-content center">
						<form method="post">

							<div class="row">
								<div class="input-field col s12">
									<i class="material-icons prefix">email</i>
									<input id="email" type="email" class="validate" name="temail">
									<label for="email">Email</label>
								  </div>
								  <div class="input-field col s12">
									<i class="material-icons prefix">https</i>
									<input id="password" type="password" class="validate" name="tpassword">
									<label for="password">Password</label>
								  </div>
							  </div>


							<input type="submit" name="" class="waves-effect waves-light grey btn" value="Login">
						</form>
					</div>
				</div>
			</div>
			
		</div>
	</div>
	</div>

	
  </body>
</html>